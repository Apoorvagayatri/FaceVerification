    "   \"metadata\": {},\n",
    "   \"source\": [\n",
    "    \"# PerspectAI Face Verification - Photo Demo\\n\",\n",
    "    \"\\n\",\n",
    "    \"This notebook allows testing of the face verification system using saved photos for both reference and test images, perfect for demonstrations.\"\n",
    "   ]\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"markdown\",\n",
    "   \"metadata\": {},\n",
    "   \"source\": [\n",
    "    \"## 1. Import Required Libraries\"\n",
    "   ]\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"code\",\n",
    "   \"execution_count\": null,\n",
    "   \"metadata\": {},\n",
    "   \"source\": [\n",
    "    \"import cv2\\n\",\n",
    "    \"import numpy as np\\n\",\n",
    "    \"import pickle\\n\",\n",
    "    \"import os\\n\",\n",
    "    \"import matplotlib.pyplot as plt\\n\",\n",
    "    \"from IPython.display import clear_output, display\\n\",\n",
    "    \"import time\\n\",\n",
    "    \"import joblib\\n\",\n",
    "    \"import warnings\\n\",\n",
    "    \"warnings.filterwarnings('ignore')\"\n",
    "   ]\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"markdown\",\n",
    "   \"metadata\": {},\n",
    "   \"source\": [\n",
    "    \"## 2. Load Pre-trained Models\"\n",
    "   ]\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"code\",\n",
    "   \"execution_count\": null,\n",
    "   \"metadata\": {},\n",
    "   \"source\": [\n",
    "    \"# Load pre-trained eigenvectors\\n\",\n",
    "    \"try:\\n\",\n",
    "    \"    with open('eig_vecsk_128sept10.pkl', 'rb') as f:\\n\",\n",
    "    \"        eig_vecs = pickle.load(f)\\n\",\n",
    "    \"    print(f\\\"Loaded eigenvectors with shape: {eig_vecs.shape}\\\")\\n\",\n",
    "    \"except Exception as e:\\n\",\n",
    "    \"    print(f\\\"Error loading eigenvectors: {e}\\\")\\n\",\n",
    "    \"    # Try alternative file name\\n\",\n",
    "    \"    try:\\n\",\n",
    "    \"        with open('eig_vecsk_128.pkl', 'rb') as f:\\n\",\n",
    "    \"            eig_vecs = pickle.load(f)\\n\",\n",
    "    \"        print(f\\\"Loaded alternative eigenvectors with shape: {eig_vecs.shape}\\\")\\n\",\n",
    "    \"    except Exception as e2:\\n\",\n",
    "    \"        print(f\\\"Error loading alternative eigenvectors: {e2}\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Load classifier (try multiple formats and models)\\n\",\n",
    "    \"classifiers = {}\\n\",\n",
    "    \"try:\\n\",\n",
    "    \"    # Try MLP with joblib\\n\",\n",
    "    \"    classifiers['mlp'] = joblib.load('mlp_classifier.pkl')\\n\",\n",
    "    \"    print(\\\"Loaded MLP classifier\\\")\\n\",\n",
    "    \"except Exception as e:\\n\",\n",
    "    \"    print(f\\\"Could not load MLP classifier: {e}\\\")\\n\",\n",
    "    \"    try:\\n\",\n",
    "    \"        # Try with pickle\\n\",\n",
    "    \"        with open('mlp_classifier.pkl', 'rb') as f:\\n\",\n",
    "    \"            classifiers['mlp'] = pickle.load(f)\\n\",\n",
    "    \"        print(\\\"Loaded MLP classifier via pickle\\\")\\n\",\n",
    "    \"    except Exception as e2:\\n\",\n",
    "    \"        print(f\\\"Could not load MLP classifier via pickle: {e2}\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"try:\\n\",\n",
    "    \"    # Try SVM\\n\",\n",
    "    \"    classifiers['svm'] = joblib.load('svc_rbf_c_10_gamma_1e-08.pkl')\\n\",\n",
    "    \"    print(\\\"Loaded SVM classifier\\\")\\n\",\n",
    "    \"except Exception as e:\\n\",\n",
    "    \"    print(f\\\"Could not load SVM classifier: {e}\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"try:\\n\",\n",
    "    \"    # Try Random Forest\\n\",\n",
    "    \"    classifiers['rf'] = joblib.load('rf_classifier10000.pkl')\\n\",\n",
    "    \"    print(\\\"Loaded Random Forest classifier\\\")\\n\",\n",
    "    \"except Exception as e:\\n\",\n",
    "    \"    print(f\\\"Could not load Random Forest classifier: {e}\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Check if we loaded any classifiers\\n\",\n",
    "    \"if not classifiers:\\n\",\n",
    "    \"    print(\\\"ERROR: No classifiers could be loaded!\\\")\\n\",\n",
    "    \"else:\\n\",\n",
    "    \"    # Set the default classifier\\n\",\n",
    "    \"    current_classifier_name = list(classifiers.keys())[0]\\n\",\n",
    "    \"    current_classifier = classifiers[current_classifier_name]\\n\",\n",
    "    \"    print(f\\\"Using {current_classifier_name} as the default classifier\\\")\"\n",
    "   ]\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"markdown\",\n",
    "   \"metadata\": {},\n",
    "   \"source\": [\n",
    "    \"## 3. Define Helper Functions\"\n",
    "   ]\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"code\",\n",
    "   \"execution_count\": null,\n",
    "   \"metadata\": {},\n",
    "   \"source\": [\n",
    "    \"def detect_face(image):\\n\",\n",
    "    \"    \\\"\\\"\\\"Detect face in an image using Viola-Jones algorithm\\\"\\\"\\\"\\n\",\n",
    "    \"    # Load the pre-trained Haar Cascade face detector\\n\",\n",
    "    \"    face_cascade = cv2.CascadeClassifier(cv2.data.haarcascades + 'haarcascade_frontalface_default.xml')\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Ensure image is in correct format\\n\",\n",
    "    \"    if image is None:\\n\",\n",
    "    \"        print(\\\"Error: Input image is None\\\")\\n\",\n",
    "    \"        return None\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Convert to grayscale if needed\\n\",\n",
    "    \"    if len(image.shape) == 3:\\n\",\n",
    "    \"        gray = cv2.cvtColor(image, cv2.COLOR_BGR2GRAY)\\n\",\n",
    "    \"    else:\\n\",\n",
    "    \"        gray = image\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Detect faces\\n\",\n",
    "    \"    faces = face_cascade.detectMultiScale(gray, 1.3, 5)\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    if len(faces) == 0:\\n\",\n",
    "    \"        print(\\\"No faces detected in the image\\\")\\n\",\n",
    "    \"        return None\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Get the largest face (assuming it's the main subject)\\n\",\n",
    "    \"    largest_face = max(faces, key=lambda rect: rect[2] * rect[3])\\n\",\n",
    "    \"    x, y, w, h = largest_face\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Extract face region\\n\",\n",
    "    \"    face_img = gray[y:y+h, x:x+w]\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Resize to 64x64 pixels (same as training)\\n\",\n",
    "    \"    face_img = cv2.resize(face_img, (64, 64))\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    return face_img, (x, y, w, h)\\n\",\n",
    "    \"\\n\",\n",
    "    \"def project_face(face_img, eig_vecs):\\n\",\n",
    "    \"    \\\"\\\"\\\"Project face onto eigenspace using 2DPCA\\\"\\\"\\\"\\n\",\n",
    "    \"    # Ensure face_img is in the right format\\n\",\n",
    "    \"    if face_img is None:\\n\",\n",
    "    \"        return None\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Normalize the image\\n\",\n",
    "    \"    face_img = face_img.astype(np.float64) / 255.0\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Project face onto eigenspace\\n\",\n",
    "    \"    projection = np.dot(face_img, eig_vecs)\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    return projection.flatten()\\n\",\n",
    "    \"\\n\",\n",
    "    \"def verify_faces(face1_features, face2_features, classifier):\\n\",\n",
    "    \"    \\\"\\\"\\\"Verify if two faces belong to the same person\\\"\\\"\\\"\\n\",\n",
    "    \"    if face1_features is None or face2_features is None:\\n\",\n",
    "    \"        return None, 0.0\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Combine features (same way as in training)\\n\",\n",
    "    \"    combined_features = np.concatenate([face1_features, face2_features])\\n\",\n",
    "    \"    combined_features = combined_features.reshape(1, -1)\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Predict using the classifier\\n\",\n",
    "    \"    try:\\n\",\n",
    "    \"        prediction = classifier.predict(combined_features)[0]\\n\",\n",
    "    \"        # Try to get probability if available\\n\",\n",
    "    \"        try:\\n\",\n",
    "    \"            if hasattr(classifier, 'predict_proba'):\\n\",\n",
    "    \"                probability = classifier.predict_proba(combined_features)[0][1] \\n\",\n",
    "    \"            else:\\n\",\n",
    "    \"                probability = float(prediction)\\n\",\n",
    "    \"        except:\\n\",\n",
    "    \"            probability = float(prediction)\\n\",\n",
    "    \"    except Exception as e:\\n\",\n",
    "    \"        print(f\\\"Error during prediction: {e}\\\")\\n\",\n",
    "    \"        return None, 0.0\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    return bool(prediction), float(probability)\\n\",\n",
    "    \"\\n\",\n",
    "    \"def load_and_process_image(image_path):\\n\",\n",
    "    \"    \\\"\\\"\\\"Load an image and prepare it for face verification\\\"\\\"\\\"\\n\",\n",
    "    \"    if not os.path.exists(image_path):\\n\",\n",
    "    \"        print(f\\\"Error: File {image_path} does not exist\\\")\\n\",\n",
    "    \"        return None, None, None\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Load the image\\n\",\n",
    "    \"    img = cv2.imread(image_path)\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    if img is None:\\n\",\n",
    "    \"        print(f\\\"Error: Could not load image {image_path}\\\")\\n\",\n",
    "    \"        return None, None, None\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Detect face\\n\",\n",
    "    \"    try:\\n\",\n",
    "    \"        face_img, face_rect = detect_face(img)\\n\",\n",
    "    \"    except Exception as e:\\n\",\n",
    "    \"        print(f\\\"Error detecting face: {e}\\\")\\n\",\n",
    "    \"        return img, None, None\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    if face_img is None:\\n\",\n",
    "    \"        return img, None, None\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Project face\\n\",\n",
    "    \"    try:\\n\",\n",
    "    \"        features = project_face(face_img, eig_vecs)\\n\",\n",
    "    \"    except Exception as e:\\n\",\n",
    "    \"        print(f\\\"Error projecting face: {e}\\\")\\n\",\n",
    "    \"        return img, face_img, None\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    return img, face_img, features\"\n",
    "   ]\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"markdown\",\n",
    "   \"metadata\": {},\n",
    "   \"source\": [\n",
    "    \"## 4. Set Reference Image\"\n",
    "   ]\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"code\",\n",
    "   \"execution_count\": null,\n",
    "   \"metadata\": {},\n",
    "   \"source\": [\n",
    "    \"def set_reference_image(image_path):\\n\",\n",
    "    \"    \\\"\\\"\\\"Set the reference image for face verification\\\"\\\"\\\"\\n\",\n",
    "    \"    global reference_img, reference_face, reference_features\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    reference_img, reference_face, reference_features = load_and_process_image(image_path)\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    if reference_img is not None:\\n\",\n",
    "    \"        # Display the reference image\\n\",\n",
    "    \"        plt.figure(figsize=(8, 6))\\n\",\n",
    "    \"        plt.imshow(cv2.cvtColor(reference_img, cv2.COLOR_BGR2RGB))\\n\",\n",
    "    \"        \\n\",\n",
    "    \"        if reference_face is not None:\\n\",\n",
    "    \"            plt.title(\\\"Reference Image - Face Detected\\\")\\n\",\n",
    "    \"            \\n\",\n",
    "    \"            # Show the processed face\\n\",\n",
    "    \"            plt.figure(figsize=(4, 4))\\n\",\n",
    "    \"            plt.imshow(reference_face, cmap='gray')\\n\",\n",
    "    \"            plt.title(\\\"Processed Reference Face\\\")\\n\",\n",
    "    \"            plt.axis('off')\\n\",\n",
    "    \"            plt.show()\\n\",\n",
    "    \"            \\n\",\n",
    "    \"            print(\\\"Reference face processed successfully!\\\")\\n\",\n",
    "    \"            return True\\n\",\n",
    "    \"        else:\\n\",\n",
    "    \"            plt.title(\\\"Reference Image - No Face Detected\\\")\\n\",\n",
    "    \"            plt.show()\\n\",\n",
    "    \"            print(\\\"Could not detect a face in the reference image.\\\")\\n\",\n",
    "    \"            return False\\n\",\n",
    "    \"    else:\\n\",\n",
    "    \"        print(\\\"Failed to load reference image.\\\")\\n\",\n",
    "    \"        return False\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Initialize reference variables\\n\",\n",
    "    \"reference_img = None\\n\",\n",
    "    \"reference_face = None\\n\",\n",
    "    \"reference_features = None\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Example: Set a reference image\\n\",\n",
    "    \"# Uncomment and change the path to use your own reference image\\n\",\n",
    "    \"# success = set_reference_image('path/to/your/reference_image.jpg')\"\n",
    "   ]\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"markdown\",\n",
    "   \"metadata\": {},\n",
    "   \"source\": [\n",
    "    \"### Set Your Reference Image\"\n",
    "   ]\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"code\",\n",
    "   \"execution_count\": null,\n",
    "   \"metadata\": {},\n",
    "   \"source\": [\n",
    "    \"# Set your reference image here\\n\",\n",
    "    \"reference_path = input(\\\"Enter the path to your reference image: \\\")\\n\",\n",
    "    \"success = set_reference_image(reference_path)\"\n",
    "   ]\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"markdown\",\n",
    "   \"metadata\": {},\n",
    "   \"source\": [\n",
    "    \"## 5. Verify Test Images\"\n",
    "   ]\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"code\",\n",
    "   \"execution_count\": null,\n",
    "   \"metadata\": {},\n",
    "   \"source\": [\n",
    "    \"def verify_test_image(image_path):\\n\",\n",
    "    \"    \\\"\\\"\\\"Verify a test image against the reference image\\\"\\\"\\\"\\n\",\n",
    "    \"    global reference_img, reference_face, reference_features\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    if reference_features is None:\\n\",\n",
    "    \"        print(\\\"No reference image set. Please set a reference image first.\\\")\\n\",\n",
    "    \"        return\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Load and process the test image\\n\",\n",
    "    \"    test_img, test_face, test_features = load_and_process_image(image_path)\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    if test_img is None:\\n\",\n",
    "    \"        print(\\\"Failed to load test image.\\\")\\n\",\n",
    "    \"        return\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Display both images side by side\\n\",\n",
    "    \"    plt.figure(figsize=(15, 7))\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Reference image\\n\",\n",
    "    \"    plt.subplot(1, 2, 1)\\n\",\n",
    "    \"    plt.imshow(cv2.cvtColor(reference_img, cv2.COLOR_BGR2RGB))\\n\",\n",
    "    \"    plt.title(\\\"Reference Image\\\")\\n\",\n",
    "    \"    plt.axis('off')\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Test image\\n\",\n",
    "    \"    plt.subplot(1, 2, 2)\\n\",\n",
    "    \"    plt.imshow(cv2.cvtColor(test_img, cv2.COLOR_BGR2RGB))\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    if test_face is None:\\n\",\n",
    "    \"        plt.title(\\\"Test Image - No Face Detected\\\", color='red')\\n\",\n",
    "    \"        plt.axis('off')\\n\",\n",
    "    \"        plt.show()\\n\",\n",
    "    \"        print(\\\"Could not detect a face in the test image.\\\")\\n\",\n",
    "    \"        return\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    if test_features is None:\\n\",\n",
    "    \"        plt.title(\\\"Test Image - Error Processing Face\\\", color='red')\\n\",\n",
    "    \"        plt.axis('off')\\n\",\n",
    "    \"        plt.show()\\n\",\n",
    "    \"        print(\\\"Error processing the face in the test image.\\\")\\n\",\n",
    "    \"        return\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Verify faces\\n\",\n",
    "    \"    prediction, probability = verify_faces(reference_features, test_features, current_classifier)\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    if prediction is None:\\n\",\n",
    "    \"        plt.title(\\\"Verification Failed\\\", color='red')\\n\",\n",
    "    \"        plt.axis('off')\\n\",\n",
    "    \"        plt.show()\\n\",\n",
    "    \"        print(\\\"Face verification process failed.\\\")\\n\",\n",
    "    \"        return\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Set title based on prediction\\n\",\n",
    "    \"    if prediction:\\n\",\n",
    "    \"        plt.title(f\\\"MATCH ✓ (Confidence: {probability:.2f})\\\", color='green', fontsize=14, fontweight='bold')\\n\",\n",
    "    \"    else:\\n\",\n",
    "    \"        plt.title(f\\\"NO MATCH ✗ (Confidence: {(1-probability):.2f})\\\", color='red', fontsize=14, fontweight='bold')\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    plt.axis('off')\\n\",\n",
    "    \"    plt.show()\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Show the processed faces\\n\",\n",
    "    \"    plt.figure(figsize=(10, 5))\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    plt.subplot(1, 2, 1)\\n\",\n",
    "    \"    plt.imshow(reference_face, cmap='gray')\\n\",\n",
    "    \"    plt.title(\\\"Processed Reference Face\\\")\\n\",\n",
    "    \"    plt.axis('off')\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    plt.subplot(1, 2, 2)\\n\",\n",
    "    \"    plt.imshow(test_face, cmap='gray')\\n\",\n",
    "    \"    plt.title(\\\"Processed Test Face\\\")\\n\",\n",
    "    \"    plt.axis('off')\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    plt.show()\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    return prediction, probability\"\n",
    "   ]\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"markdown\",\n",
    "   \"metadata\": {},\n",
    "   \"source\": [\n",
    "    \"### Verify Individual Images\"\n",
    "   ]\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"code\",\n",
    "   \"execution_count\": null,\n",
    "   \"metadata\": {},\n",
    "   \"source\": [\n",
    "    \"# Example: Verify a test image\\n\",\n",
    "    \"test_path = input(\\\"Enter the path to your test image: \\\")\\n\",\n",
    "    \"result = verify_test_image(test_path)\"\n",
    "   ]\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"markdown\",\n",
    "   \"metadata\": {},\n",
    "   \"source\": [\n",
    "    \"## 6. Batch Verification (Multiple Tests)\"\n",
    "   ]\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"code\",\n",
    "   \"execution_count\": null,\n",
    "   \"metadata\": {},\n",
    "   \"source\": [\n",
    "    \"def verify_batch_images(positive_paths, negative_paths):\\n\",\n",
    "    \"    \\\"\\\"\\\"Verify multiple positive and negative test images\\\"\\\"\\\"\\n\",\n",
    "    \"    global reference_img, reference_face, reference_features\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    if reference_features is None:\\n\",\n",
    "    \"        print(\\\"No reference image set. Please set a reference image first.\\\")\\n\",\n",
    "    \"        return\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    results = {}\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Process positive examples\\n\",\n",
    "    \"    print(\\\"\\\\n=== Testing Positive Examples (Should Match) ===\\\\n\\\")\\n\",\n",
    "    \"    for i, path in enumerate(positive_paths):\\n\",\n",
    "    \"        print(f\\\"Testing positive example {i+1}/{len(positive_paths)}: {path}\\\")\\n\",\n",
    "    \"        prediction, probability = verify_test_image(path)\\n\",\n",
    "    \"        results[path] = {'expected': True, 'prediction': prediction, 'probability': probability}\\n\",\n",
    "    \"        print()\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Process negative examples\\n\",\n",
    "    \"    print(\\\"\\\\n=== Testing Negative Examples (Should Not Match) ===\\\\n\\\")\\n\",\n",
    "    \"    for i, path in enumerate(negative_paths):\\n\",\n",
    "    \"        print(f\\\"Testing negative example {i+1}/{len(negative_paths)}: {path}\\\")\\n\",\n",
    "    \"        prediction, probability = verify_test_image(path)\\n\",\n",
    "    \"        results[path] = {'expected': False, 'prediction': prediction, 'probability': probability}\\n\",\n",
    "    \"        print()\\n\",\n",
